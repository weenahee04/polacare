{
  "name": "polacare-backend",
  "version": "1.0.0",
  "description": "POLACARE Backend API Server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/server.js",
    "setup": "npm install && npx prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "seed": "tsx src/db/seed.ts",
    "seed:staff": "tsx src/db/seedStaff.ts",
    "seed:test": "tsx tests/seed-test-data.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "backup": "tsx src/scripts/backup.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:smoke": "jest --selectProjects=smoke",
    "test:integration": "jest --selectProjects=integration",
    "test:unit": "jest --selectProjects=unit",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:api": "tsx scripts/test-api.ts",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop polacare-backend",
    "pm2:restart": "pm2 restart polacare-backend",
    "pm2:logs": "pm2 logs polacare-backend"
  },
  "keywords": [
    "healthcare",
    "patient-portal",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@google/generative-ai": "^0.2.1",
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "sharp": "^0.33.1",
    "twilio": "^4.20.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0"
  }
}
